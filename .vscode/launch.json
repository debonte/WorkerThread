{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "node",
        "runtimeArgs": [
          "--inspect-brk",
          "${workspaceFolder}/main.js"
        ],
        "console": "integratedTerminal",
        "skipFiles": [
          "<node_internals>/**"
        ]
      },
      {
        "name": "Launch without debugging",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "node",
        "runtimeArgs": [
          "--inspect-brk=9229",
          "${workspaceFolder}/main.js"
        ],
        "skipFiles": [
          "<node_internals>/**"
        ]
      },
      {
        "name": "Attach",
        "type": "node",
        "request": "attach",
        "port": 9229
      }
    ]
  }
  